@model APPMVC.NET.Models.PlanetModel

<h1>@Model.Name</h1>

<div class="card w-70">
    <img src="~/planet/@(Model.Name).jpg" alt="">
    <div class="card-body">
        <h5>@Model.VnName</h5>
        <div class="card-text">
            @Model.Content
        </div>
        <hr>
        <a class="card-link" asp-action="Index">Các hành tinh</a>
    </div>
</div>